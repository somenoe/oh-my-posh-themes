{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "#0ea5e9",
                    "properties": {
                        "time_format": "Monday"
                    },
                    "style": "diamond",
                    "template": "<#ffffff>\uf5ef</> {{ .CurrentDate | date .Format }}",
                    "type": "time"
                },
                {
                    "foreground": "#737373",
                    "style": "plain",
                    "template": " at ",
                    "type": "session"
                },
                {
                    "foreground": "#ea580c",
                    "properties": {
                        "time_format": "03:04:05"
                    },
                    "style": "diamond",
                    "template": "{{ .CurrentDate | date .Format }}",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#0ea5e9",
                    "style": "plain",
                    "template": "<#ffffff>\uf508</> {{ .UserName }}",
                    "type": "session"
                },
                {
                    "foreground": "#737373",
                    "style": "plain",
                    "template": " from ",
                    "type": "session"
                },
                {
                    "foreground": "#0ea5e9",
                    "style": "plain",
                    "template": "<#ffffff>\ufcbe</> {{ .HostName }} ",
                    "type": "session"
                },
                {
                    "properties": {
                        "root_icon": "\uf292"
                    },
                    "style": "diamond",
                    "template": "\uf0e7",
                    "type": "root"
                },
                {
                    "foreground": "#f43f5e",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\uf692</> {{ .StashCount }}{{ end }}]",
                    "type": "git"
                },
                {
                    "foreground": "#f43f5e",
                    "style": "plain",
                    "template": "[{{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }}]",
                    "type": "aws"
                },
                {
                    "foreground": "#f43f5e",
                    "style": "plain",
                    "template": "[{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
                    "type": "kubectl"
                },
                {
                    "foreground": "#f43f5e",
                    "style": "plain",
                    "template": "[\uf0e7]",
                    "type": "root"
                }
            ],
            "type": "prompt"
        },
        {
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#ea580c",
                    "style": "diamond",
                    "template": "<#ffffff>\uf85a</> RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB ",
                    "type": "sysinfo"
                },
                {
                    "foreground": "#ea580c",
                    "properties": {
                        "style": "dallas",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": "- <#ffffff>\uf252</> {{ .FormattedMs }}s ",
                    "type": "executiontime"
                },
                {
                    "foreground": "#f43f5e",
                    "style": "powerline",
                    "template": "- <#ffffff>\ufc8d</> Error, check your command ",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#737373",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "<#65a30d>{{ .Path }}</> ",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#737373",
                    "style": "plain",
                    "template": "\ufb0c",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
